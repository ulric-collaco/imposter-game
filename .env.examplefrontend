# Frontend environment variables example
# Copy to frontend/.env or configure via your hosting provider.

# Supabase client configuration
VITE_SUPABASE_URL=
VITE_SUPABASE_ANON_KEY=

# WebSocket server URL
# In development, the app defaults to ws://localhost:8080 if unset
VITE_WEBSOCKET_URL=

# Optional: enable REST/Realtime fallback when WebSocket is unavailable
# Defaults to disabled unless pointing to localhost; set to true to force enable
VITE_ENABLE_SUPABASE_FALLBACK=false
