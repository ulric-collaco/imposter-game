# Backend environment variables example
# Copy to .env in the backend service directory or use these values in your CI/CD environment

# Supabase service configuration
SUPABASE_URL=
SUPABASE_SERVICE_KEY=

# Server configuration
PORT=8080
NODE_ENV=development
# Comma-separated list of allowed origins for CORS/WebSocket in non-test envs
ALLOWED_ORIGINS=http://localhost:5173,http://localhost:3000
